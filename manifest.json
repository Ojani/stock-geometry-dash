{
  "name": "Chart Dart",
  "description": "Get a geometry-dash style dart to reveal the price graph of stocks!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "./popup/index.html",
    "default_icon": "chart-dart-icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}